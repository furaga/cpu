## -*- coding: utf-8 -*-
<%inherit file="skeleton.tmp"/>

entity decode is
<%include file="decode_entity.tmp"/>
end decode;

architecture RTL of decode is


begin
	process(CLK_DC)
	begin
		if rising_edge(CLK_DC) then
			IR <= PROM_OUT;
			FP <= FP_OUT(19 downto 0);
			LR <= LINK_OUT;

			if PROM_OUT(31 downto 26)="000001" and
			   PROM_OUT(5 downto 0)="000000" then
				INPUT_FLAG <= '1';
			else
				INPUT_FLAG <= '0';
			end if;

		end if;
	end process;
end RTL;
<%namespace name="oc" file="macro.tmp"/>
