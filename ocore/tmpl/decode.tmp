entity decode is
<%include file="decode_entity.tmp"/>
end decode;

architecture RTL of decode is


begin
	process(CLK_DC)
	begin
		if rising_edge(CLK_DC) then
			IR <= PROM_OUT;
			FP <= FP_OUT(19 downto 0);
			LR <= LINK_OUT(31 downto 0);
		end if;
	end process;
end RTL;
